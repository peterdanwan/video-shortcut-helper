{
  "manifest_version": 3,
  "name": "Video Keyboard Controls",
  "version": "0.0.1",
  "description": "Adds keyboard controls to video players",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}